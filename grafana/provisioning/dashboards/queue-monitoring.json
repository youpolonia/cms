{
  "title": "Queue Monitoring",
  "panels": [
    {
      "title": "Queue Lengths",
      "type": "graph",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "redis_queue_length{queue=\"high\"}",
          "legendFormat": "High Priority"
        },
        {
          "expr": "redis_queue_length{queue=\"medium\"}",
          "legendFormat": "Medium Priority"
        },
        {
          "expr": "redis_queue_length{queue=\"low\"}",
          "legendFormat": "Low Priority"
        }
      ]
    },
    {
      "title": "Worker Utilization",
      "type": "stat",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(laravel_queue_worker_busy[1m])) / sum(laravel_queue_worker_count) * 100",
          "legendFormat": "Utilization %"
        }
      ]
    },
    {
      "title": "Job Processing Rate",
      "type": "graph",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(laravel_queue_jobs_processed_total[1m])",
          "legendFormat": "Jobs/sec"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "graph",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(laravel_queue_jobs_failed_total[1m])",
          "legendFormat": "Errors/sec"
        }
      ]
    }
  ]
}
{
    "name": "Contact", 
    "content": {
        "version": "1.0",
        "content": [
            {
                "type": "section",
                "attrs": {
                    "_pattern": "hero",
                    "padding": {"top": 100, "right": 0, "bottom": 100, "left": 0},
                    "padding__phone": {"top": 80, "bottom": 80, "left": 20, "right": 20},
                    "background_color": "#f8fafc"
                },
                "children": [
                    {
                        "type": "row",
                        "attrs": {"columns": "1", "max_width": 800, "text_align": "center"},
                        "children": [
                            {
                                "type": "column",
                                "attrs": {"width": "1"},
                                "children": [
                                    {
                                        "type": "heading",
                                        "attrs": {"text": "Ready to Start Something Amazing?", "header_level": "h1", "font_family": "Space Grotesk", "font_size": 52, "font_size__phone": 36, "font_weight": "700", "line_height": 1.1, "text_color": "#0f172a", "margin_bottom": 24},
                                        "id": "agcy_contact_hero_title"
                                    },
                                    {
                                        "type": "text",
                                        "attrs": {"content": "<p>Let's discuss your project and explore how we can bring your vision to life with bold design and strategic thinking.</p>", "font_family": "Inter", "font_size": 20, "line_height": 1.6, "text_color": "#64748b", "margin_bottom": 0},
                                        "id": "agcy_contact_hero_desc"
                                    }
                                ],
                                "id": "agcy_contact_hero_column"
                            }
                        ],
                        "id": "agcy_contact_hero_row"
                    }
                ],
                "id": "agcy_contact_hero_section"
            },
            {
                "type": "section",
                "attrs": {
                    "_pattern": "contact_form",
                    "padding": {"top": 120, "right": 0, "bottom": 120, "left": 0},
                    "padding__phone": {"top": 80, "bottom": 80, "left": 20, "right": 20},
                    "background_color": "#ffffff"
                },
                "children": [
                    {
                        "type": "row",
                        "attrs": {"columns": "1_2,1_2", "max_width": 1280, "gap": 80},
                        "children": [
                            {
                                "type": "column",
                                "attrs": {"width": "1_2"},
                                "children": [
                                    {
                                        "type": "text",
                                        "attrs": {"content": "<p style=\"color:#f43f5e;font-weight:600;font-size:14px;text-transform:uppercase;letter-spacing:1px;margin-bottom:16px\">Let's Talk</p>", "font_family": "Inter"},
                                        "id": "agcy_contact_label"
                                    },
                                    {
                                        "type": "heading",
                                        "attrs": {"text": "Tell Us About Your Project", "header_level": "h2", "font_family": "Space Grotesk", "font_size": 36, "font_size__phone": 28, "font_weight": "700", "line_height": 1.2, "text_color": "#0f172a", "margin_bottom": 24},
                                        "id": "agcy_contact_title"
                                    },
                                    {
                                        "type": "text",
                                        "attrs": {"content": "<p>Whether you need a complete brand overhaul, a cutting-edge website, or a strategic marketing campaign, we're here to help. Fill out the form and we'll get back to you within 24 hours.</p>", "font_family": "Inter", "font_size": 16, "line_height": 1.7, "text_color": "#64748b", "margin_bottom": 32},
                                        "id": "agcy_contact_desc"
                                    },
                                    {
                                        "type": "contact_form",
                                        "attrs": {
                                            "fields": [
                                                {"name": "name", "label": "Name", "type": "text", "required": true, "width": "full"},
                                                {"name": "email", "label": "Email", "type": "email", "required": true, "width": "half"},
                                                {"name": "company", "label": "Company", "type": "text", "required": false, "width": "half"},
                                                {"name": "budget", "label": "Budget Range", "type": "select", "options": ["$5k - $15k", "$15k - $30k", "$30k - $50k", "$50k+", "Let's discuss"], "required": false, "width": "half"},
                                                {"name": "timeline", "label": "Timeline", "type": "select", "options": ["ASAP", "1-2 months", "3-6 months", "6+ months", "Just exploring"], "required": false, "width": "half"},
                                                {"name": "services", "label": "Services Needed", "type": "checkbox", "options": ["Web Design", "Brand Identity", "Digital Marketing", "UX/UI Design", "Strategy"], "required": false, "width": "full"},
                                                {"name": "message", "label": "Tell us about your project", "type": "textarea", "required": true, "width": "full"}
                                            ],
                                            "submit_text": "Send Project Details",
                                            "button_style": "solid",
                                            "button_color": "#f43f5e",
                                            "success_message": "Thanks! We'll review your project and get back to you within 24 hours."
                                        },
                                        "id": "agcy_contact_form"
                                    }
                                ],
                                "id": "agcy_contact_col1"
                            },
                            {
                                "type": "column",
                                "attrs": {"width": "1_2"},
                                "children": [
                                    {
                                        "type": "text",
                                        "attrs": {"content": "<p style=\"color:#f43f5e;font-weight:600;font-size:14px;text-transform:uppercase;letter-spacing:1px;margin-bottom:16px\">Get In Touch</p>", "font_family": "Inter"},
                                        "id": "agcy_info_label"
                                    },
                                    {
                                        "type": "heading",
                                        "attrs": {"text": "Let's Start the Conversation", "header_level": "h2", "font_family": "Space Grotesk", "font_size": 36, "font_size__phone": 28, "font_weight": "700", "line_height": 1.2, "text_color": "#0f172a", "margin_bottom": 32},
                                        "id": "agcy_info_title"
                                    },
                                    {
                                        "type": "text",
                                        "attrs": {"content": "<div style=\"margin-bottom:32px\"><h4 style=\"margin:0 0 8px;font-family:'Space Grotesk';font-size:18px;color:#0f172a\">‚úâÔ∏è Email Us</h4><p style=\"margin:0;color:#64748b;line-height:1.6\"><a href=\"mailto:hello@agency.com\" style=\"color:#f43f5e;text-decoration:none\">hello@agency.com</a><br>We typically respond within a few hours</p></div><div style=\"margin-bottom:32px\"><h4 style=\"margin:0 0 8px;font-family:'Space Grotesk';font-size:18px;color:#0f172a\">üìû Call Us</h4><p style=\"margin:0;color:#64748b;line-height:1.6\"><strong>(555) CREATIVE</strong><br>Mon-Fri, 9 AM - 6 PM EST</p></div><div style=\"margin-bottom:32px\"><h4 style=\"margin:0 0 8px;font-family:'Space Grotesk';font-size:18px;color:#0f172a\">üìç Visit Our Studio</h4><p style=\"margin:0;color:#64748b;line-height:1.6\">123 Creative Boulevard<br>Design District<br>New York, NY 10001</p></div>", "font_family": "Inter", "font_size": 16},
                                        "id": "agcy_contact_info"
                                    },
                                    {
                                        "type": "text",
                                        "attrs": {"content": "<div style=\"background:#f8fafc;padding:24px;border-radius:12px;border-left:4px solid #f43f5e\"><h4 style=\"margin:0 0 12px;font-family:'Space Grotesk';font-size:18px;color:#0f172a\">üöÄ Free Strategy Call</h4><p style=\"margin:0;color:#64748b;font-size:14px;line-height:1.6\">Book a 30-minute strategy call to discuss your project goals, challenges, and how we can help.</p><p style=\"margin:12px 0 0\"><a href=\"#\" style=\"color:#f43f5e;font-weight:600;text-decoration:none\">Schedule Your Call ‚Üí</a></p></div>", "font_family": "Inter"},
                                        "id": "agcy_strategy_call"
                                    }
                                ],
                                "id": "agcy_contact_col2"
                            }
                        ],
                        "id": "agcy_contact_row"
                    }
                ],
                "id": "agcy_contact_section"
            }
        ]
    }
}
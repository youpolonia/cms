  "native_mode": "dark",
  "colors_alt": {
    "--background": "#faf5ef",
    "--surface": "#ffffff",
    "--color-surface": "#ffffff",
    "--text": "#2d1f12",
    "--text-muted": "#6b5a4a",
    "--color-border": "rgba(0,0,0,0.1)"
  }

                                    },
                                    {
                                        "type": "heading",
                                        "attrs": {"text": "Exceptional Cuisine, Unforgettable Experience", "header_level": "h1", "font_family": "Playfair Display", "font_size": 56, "font_size__tablet": 44, "font_size__phone": 36, "font_weight": "700", "line_height": 1.1, "text_color": "#ffffff", "margin_bottom": 24},
                                        "id": "rest_hero_title"
                                    },

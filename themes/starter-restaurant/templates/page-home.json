                                                "children": [
                                                    {
                                                        "type": "button",
                                                        "attrs": {"text": "Reserve Your Table", "link_url": "#contact", "button_style": "solid", "font_size": 16, "font_weight": "600", "border_radius": {"top_left": 8, "top_right": 8, "bottom_right": 8, "bottom_left": 8}, "background_color": "#d97706", "text_color": "#ffffff", "background_color__hover": "#92400e", "padding": {"top": 16, "right": 32, "bottom": 16, "left": 32}},
